{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-native",
    "strict": false,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "app",
    "app.config.js",
    "expo-env.d.js"
  ],
  "exclude": ["node_modules"]
}
